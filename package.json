{
  "name": "ssot-ui",
  "version": "0.1.0",
  "author": "Sameer Haque <sameer.haque@kinarasystems.com> & John Campbell <john.campbell@kinarasystems.com>",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/jsachdev5gllc/ssot-ui/src/master/"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint src --ext .ts,.tsx --cache",
    "lint:fix": "eslint src --fix && yarn format",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src",
    "up": "yarn upgrade-interactive",
    "up-latest": "yarn up --latest",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.0",
    "@chakra-ui/react": "^1.8.8",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@fontsource/inter": "^4.5.10",
    "@fontsource/raleway": "^4.5.8",
    "axios": "^0.27.2",
    "cesium": "^1.94.0",
    "framer-motion": "^5.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.3.0",
    "reflect-metadata": "^0.1.13",
    "sass": "^1.51.0",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^16.2.4",
    "@platypii/cesium": "^1.94.0",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "^14.2.0",
    "@types/node": "^17.0.31",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.16",
    "@types/react-helmet": "^6.1.5",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vitejs/plugin-react": "^1.3.2",
    "commitlint": "^16.2.4",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-testing-library": "^5.5.1",
    "husky": "^8.0.1",
    "jsdom": "^19.0.0",
    "lint-staged": "^13.0.0",
    "prettier": "^2.6.2",
    "standard-version": "^9.5.0",
    "typescript": "^4.7.2",
    "vite": "^2.9.9",
    "vitest": "^0.13.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ]
}
